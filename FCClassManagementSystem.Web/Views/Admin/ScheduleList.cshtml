@model IEnumerable<FCCClassManagementSystem.Domain.Entities.Schedule>


<!DOCTYPE html>

<link href="~/Content/bootstrap.min.css" rel="stylesheet" />
<link href="~/Content/bootstrap-theme.min.css" rel="stylesheet" />

<div class="panel panel-default col-md-8 col-md-offset-2">

@if (Model.Count() != 0)
{
    foreach (var item in Model)
    {
        <div class="panel-heading">@item.ScheduleID</div>
        <div class="panel-body">
            <p>@item.FitnessClass.Name</p>            
            <p>@item.Date</p>
            <p>@item.Capacity</p>
            <p>@item.Status</p>
        </div>
    }
}
</div>    